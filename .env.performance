# Configurações de Performance
# Adicione estas variáveis de ambiente para otimizar o sistema

# Modo debug (apenas para desenvolvimento)
DEBUG_MODE=false

# Cache TTL (segundos)
CACHE_TTL=300

# Limites de consulta
MAX_AGENTES_CONSULTA=10
MAX_TRANSFERENCIAS_CONSULTA=5
MAX_RESULTADOS_HISTORICO=20

# Timeouts de API (segundos)
API_TIMEOUT=5
GEOCODING_TIMEOUT=3

# Configurações de memória
MAX_CACHE_SIZE=500
MAX_LOGS_SISTEMA=500
